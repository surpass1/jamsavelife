
<article class="section">
    <div class="container">
        <div class="col-md-9 contents">
            <div class="projects projects-list">
                <ul class="media-list">
                    <li class="media animated bounceIn in" data-animation="bounceIn" data-delay="0">
                        <% @kabale.each do |h|%>
                            <li class="media animated bounceIn in" data-animation="bounceIn" data-delay="0">
                                <figure class="hover-strip pull-left">
                                    <a class="pull-left">
                                        <%= image_tag h.avatar(:medium)%>
                                    </a>
                                    <div class="date-tag">JAM</div>
                                </figure>
                                <div class="media-body">
                                    <h4 class="media-heading"><%= h.title %></h4>
                                    <p><%= h.description %></p>
                                </div>
                            </li>
                        <% end %>
                    </li>
                    
                </ul>
            </div>
           
        </div>
        
        <div class="col-md-3 sidebar">
            
            <div class="widget widget_categories animated fadeInUp in" data-animation="fadeInUp" data-delay="0">
                <header class="heading">
                    <h4>Categories</h4>
                </header>
                <ul>
                    <li><a href="#">Nursing school</a><i class="icon-map-marker"></i></li>
                    <li><a href="#">School at Rubanda</a><i class="icon-map-marker"></i></li>
                    <li><a href="#">Kishanje</a><i class="icon-map-marker"></i></li>
                    <li><a href="#">Hoima</a><i class="icon-map-marker"></i></li>
                    <li><a href="#">Kampala</a><i class="icon-map-marker"></i></li>
                </ul>
            </div>
        </div>
    </article>
