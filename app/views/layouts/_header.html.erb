
<header class="site-header header-home" >
    <!-- tp-bar -->
    <div class="tp-bar">
        <div class="container">
          <div class="row">
            <!-- logo -->
            <div class="logo image">
                <a href="#"><%= image_tag("logo.png", :size=> '110x110')%></a>
                <a><h1 class="brand" style="color:#fff;">JUNA AMAGARA MINISTRIES</h1></a> 
            </div>  
           
            <!-- counter ends -->
            <div class="counter">
                  <p class="lead btn btn-lg btn-block btn-warning ">**Donate**
                    <a href="#"><%= image_tag("logo.png", :size=> '30x30')%></a>
                  </p>
                <div class="">
                      <p class="notice"><%= notice %></p>
